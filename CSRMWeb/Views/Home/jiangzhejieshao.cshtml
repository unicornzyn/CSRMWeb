@model List<CSRMDAL.jiangzhejieshao>
@{
    ViewBag.Title = "jiangzhejieshao";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="jiangzhejieshao-main">
    <div class="jiangzhejieshao-main-top1">讲者介绍</div>
    <div class="jiangzhejieshao-main-top2">
        @foreach (var o in Model)
        {
            <div class="jiangzhejieshao-main-top2-top index-content">
                <div class="jiangzhejieshao-main-top2-right">
                    <img src="/upload/@o.img" />
                </div>
                <div class="jiangzhejieshao-main-top2-left">
                    <p><span>姓名：</span><span>@o.name</span><span class="jiangzhejieshao-main-top2-left-span">教授</span></p>
                    <p><span>单位：</span><span>@o.unitname</span></p>
                    <p>讲解者简介：</p>
                    <p>@o.info</p>
                </div>
            </div>
        }
        <div class="jiangzhejieshao-main-top1">讲者一览</div>
        <div class="jiangzhejieshao-main-text"><span>主会场主席：</span><span>主会场主席：</span>孙莹璞教授</div>
        <div class="jiangzhejieshao-main-text"><span>分会场主席：</span><span>黄国宁教授、</span><span>冯云教授、</span><span>沈浣教授、</span><span>周灿权教授</span></div>
        <div class="jiangzhejieshao-main-text"><span>福州会场：：</span><span>黄国宁教授、</span><span>冯云教授、</span><span>沈浣教授、</span><span>周灿权教授</span></div>
        <div class="jiangzhejieshao-main-text"><span>昆明会场：</span><span>黄国宁教授、</span><span>冯云教授、</span><span>沈浣教授、</span><span>周灿权教授</span></div>
        <div class="jiangzhejieshao-main-text"><span>上海会场：</span><span>黄国宁教授、</span><span>冯云教授、</span><span>沈浣教授、</span><span>周灿权教授</span></div>
        <div class="jiangzhejieshao-main-text"><span>北京会场：</span><span>黄国宁教授、</span><span>冯云教授、</span><span>沈浣教授、</span><span>周灿权教授</span></div>
        <div class="jiangzhejieshao-main-text"><span>太原会场：</span><span>黄国宁教授、</span><span>冯云教授、</span><span>沈浣教授、</span><span>周灿权教授</span></div>
    </div>
</div>

